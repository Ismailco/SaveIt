{
  "manifest_version": 3,
  "name": "SaveIt - Bookmark Manager",
  "version": "0.1.2",
  "description": "A simple bookmark manager that overrides the new tab page",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage", "tabs"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "1024": "icons/icon1024.png"
  }
}
